<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="disease_report_count_wizard_view_form" model="ir.ui.view">
        <field name="name">disease.report.count.wizard.view.form</field>
        <field name="model">disease.report.count.wizard</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="result_disease_report" readonly="1" force_save="1" nolabel="1"/>
                    </group>
                </sheet>
                <footer>
                    <button string="Cancel" special="cancel" type="object"
                            class="btn btn-secondary oe_inline"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>

