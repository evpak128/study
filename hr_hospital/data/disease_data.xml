<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="hr_hospital_disease_data_tuberculosis" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Tuberculosis</field>
        <field name="category_id" eval="ref('hr_hospital.hr_hospital_disease_category_infectious')"/>
    </record>

    <record id="hr_hospital_disease_data_diabetes" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Diabetes</field>
        <field name="category_id" eval="ref('hr_hospital_disease_category_non_infectious')"/>
    </record>

    <record id="hr_hospital_disease_data_ebola" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Ebola</field>
        <field name="category_id" eval="ref('hr_hospital.hr_hospital_disease_category_infectious')"/>
    </record>

    <record id="hr_hospital_disease_data_covid_19" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Covid-19</field>
        <field name="category_id" eval="ref('hr_hospital.hr_hospital_disease_category_infectious')"/>
    </record>

    <record id="hr_hospital_disease_data_depression" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Depression</field>
        <field name="category_id" eval="ref('hr_hospital.hr_hospital_disease_category_mental')"/>
    </record>

    <record id="hr_hospital_disease_data_industrial_injury" model="hr.hospital.disease" forcecreate="True">
        <field name="name">Industrial injury</field>
        <field name="category_id" eval="ref('hr_hospital.hr_hospital_disease_category_physical')"/>
    </record>

</odoo>
