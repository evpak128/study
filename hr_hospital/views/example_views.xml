<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="hr_hospital_example_tree" model="ir.ui.view">
        <field name="name">hr.hospital.example.tree</field>
        <field name="model">hr.hospital.example</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>

            </tree>
        </field>
    </record>

    <record id="hr_hospital_example_form" model="ir.ui.view">
        <field name="name">hr.hospital.example.form</field>
        <field name="model">hr.hospital.example</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="hr_hospital_example_act_window" model="ir.actions.act_window">
        <field name="name">Example</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">hr.hospital.example</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="hr_hospital_example_menu"
              parent="hr_hospital.hr_hospital_research_main_menu"
              action="hr_hospital.hr_hospital_example_act_window"
              name="Example" sequence="200"/>
</odoo>

